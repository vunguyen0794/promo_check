{
  "version": 2,
  "builds": [{ "src": "server.js", "use": "@vercel/node" }],
  "routes": [{ "src": "/(.*)", "dest": "server.js", "methods": ["GET","POST","PUT","DELETE","PATCH","OPTIONS","HEAD"] }],
  "functions": {
    "server.js": {
      "includeFiles": ["views/**", "public/**"]
    }
  },
  "env": { "NODE_ENV": "production" }
}
